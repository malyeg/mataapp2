{
  "name": "mataapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "a": "react-native run-android",
    "i": "react-native run-ios",
    "iphone": "react-native run-ios --device=\"iphone1\"",
    "start": "react-native start",
    "clean": "concurrently 'cd android;./gradlew clean' 'cd ios;pod cache clean --all'",
    "uninstall": "adb uninstall com.mataapp",
    "debug": "adb shell input keyevent 82",
    "all": "react-native run-android & react-native run-ios",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "bump-patch": "yarn version patch --new-version --no-git-tag-version && fastlane bump",
    "bump-minor": "yarn version minor --new-version --no-git-tag-version && fastlane bump",
    "bump-major": "yarn version major --new-version --no-git-tag-version && fastlane bump"
  },
  "dependencies": {
